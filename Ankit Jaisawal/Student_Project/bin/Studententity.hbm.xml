<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
<!-- create -->
<hibernate-mapping>
   <class name = "com.hibernateentityclass.Studententity" table = "student">
             
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "Name" column = "name" type = "string"/>
      <property name = "rollno" column = "rollno" type = "string"/>
      <property name=   "course"  column="course" type="string"/>
      <property name="session"  column="session" type="string"/>
      <property name = "phone" column = "Phone" type = "long"/>
      
   </class>
</hibernate-mapping>
